
import java.util.Scanner;

class PosNeg {

	public static void main(String [] args) {
	
		int num;
		Scanner input = new Scanner(System.in);
		
		num = input.nextInt(); 
		
		if (num < 0)
			System.out.println("Number " + num + " is Negative number.");
		else
			System.out.println("Number " + num + " is Positive number.");
	
	}

}